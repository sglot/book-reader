{
  "title": "book-reader",
  "version": "1.0.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "cross-env NODE_ENV=development node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "test": "cross-env NODE_ENV=development TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/**/*.ts",
    "format": "cross-env NODE_ENV=format TS_NODE_FILES=true mocha --exit --require ts-node/register --colors run/run.format.html.test.ts",
    "format1": "cross-env NODE_ENV=format TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/format.html.test.ts",
    "bookmarks": "cross-env NODE_ENV=format TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/bookmark.repository.test.ts",
    "coverage": "nyc npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.17",
    "@types/polka": "^0.5.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.3.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "fs-extra": "^9.0.1",
    "polka": "next",
    "sirv": "^1.0.0"
  }
}
